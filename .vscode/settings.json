{
    "autopep8.args": [
        "--max-line-length=150"
    ],
    "python.languageServer": "None",
    "python.analysis.diagnosticSeverityOverrides": {
    
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "variable": "#9CDCFE",
            "parameter": "#9CDCFE",
            "property": "#9CDCFE",
            "function": "#DCDCAA",
            "method": "#DCDCAA",
            "class": "#4EC9B0",
            "interface": "#B8D7A3",
            "namespace": "#569CD6",
            "enum": "#B8D7A3",
            "type": "#4EC9B0",
            "keyword": "#569CD6",
            "string": "#CE9178",
            "number": "#B5CEA8",
            "comment": "#6A9955"
        }
    }
}